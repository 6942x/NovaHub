    UpdatePerformanceButtons()
end)

local function ApplyShader1()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
        OriginalShaders.Bloom = Lighting:FindFirstChildOfClass("BloomEffect")
        OriginalShaders.Blur = Lighting:FindFirstChildOfClass("BlurEffect")
    end
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") or effect:IsA("BlurEffect") then
            effect:Destroy()
        end
    end
    
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Contrast = 0.3
    colorCorrection.Saturation = 0.4
    colorCorrection.Brightness = 0.2
    colorCorrection.TintColor = Color3.fromRGB(255, 240, 220)
    colorCorrection.Parent = Lighting
    
    local bloom = Instance.new("BloomEffect")
    bloom.Intensity = 0.8
    bloom.Size = 32
    bloom.Threshold = 0.7
    bloom.Parent = Lighting
    
    local blur = Instance.new("BlurEffect")
    blur.Size = 2
    blur.Parent = Lighting
    
    CurrentShader = 1
    ShadersStatus.Text = "Current: Vibrant\n\nEnhanced colors, brightness and bloom"
    CreateNotification("üåà Shader Applied", "Vibrant colors with bloom", 2)
end

local function ApplyShader2()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
        OriginalShaders.Bloom = Lighting:FindFirstChildOfClass("BloomEffect")
    end
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") then
            effect:Destroy()
        end
    end
    
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Contrast = 0.6
    colorCorrection.Saturation = -0.2
    colorCorrection.Brightness = 0.3
    colorCorrection.TintColor = Color3.fromRGB(220, 220, 200)
    colorCorrection.Parent = Lighting
    
    CurrentShader = 2
    ShadersStatus.Text = "Current: Cartoon\n\nHigh contrast, desaturated colors"
    CreateNotification("üåà Shader Applied", "Cartoon style activated", 2)
end

local function ApplyShader3()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
        OriginalShaders.Atmosphere = Lighting:FindFirstChildOfClass("Atmosphere")
    end
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") then
            effect:Destroy()
        end
    end
    
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Contrast = 0.2
    colorCorrection.Saturation = -0.4
    colorCorrection.Brightness = 0.1
    colorCorrection.TintColor = Color3.fromRGB(180, 200, 255)
    colorCorrection.Parent = Lighting
    
    local atmosphere = Instance.new("Atmosphere")
    atmosphere.Density = 0.3
    atmosphere.Offset = 0.5
    atmosphere.Color = Color3.fromRGB(200, 220, 255)
    atmosphere.Decay = Color3.fromRGB(100, 120, 180)
    atmosphere.Glare = 0
    atmosphere.Haze = 0
    atmosphere.Parent = Lighting
    
    CurrentShader = 3
    ShadersStatus.Text = "Current: Cold\n\nCool blue tones with atmosphere"
    CreateNotification("üåà Shader Applied", "Cold filter with atmosphere", 2)
end

local function ApplyShader4()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
        OriginalShaders.SunRays = Lighting:FindFirstChildOfClass("SunRaysEffect")
    end
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") or effect:IsA("SunRaysEffect") then
            effect:Destroy()
        end
    end
    
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Contrast = 0.25
    colorCorrection.Saturation = 0.2
    colorCorrection.Brightness = 0.15
    colorCorrection.TintColor = Color3.fromRGB(255, 230, 200)
    colorCorrection.Parent = Lighting
    
    local sunRays = Instance.new("SunRaysEffect")
    sunRays.Intensity = 0.3
    sunRays.Spread = 0.1
    sunRays.Parent = Lighting
    
    CurrentShader = 4
    ShadersStatus.Text = "Current: Warm\n\nWarm orange tones with sun rays"
    CreateNotification("üåà Shader Applied", "Warm filter with sun rays", 2)
end

local function ApplyShader5()
    local Lighting = game:GetService("Lighting")
    
    if not OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection = Lighting:FindFirstChildOfClass("ColorCorrectionEffect")
        OriginalShaders.Bloom = Lighting:FindFirstChildOfClass("BloomEffect")
    end
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") then
            effect:Destroy()
        end
    end
    
    local colorCorrection = Instance.new("ColorCorrectionEffect")
    colorCorrection.Contrast = 0.5
    colorCorrection.Saturation = -1.0
    colorCorrection.Brightness = -0.1
    colorCorrection.TintColor = Color3.fromRGB(128, 128, 128)
    colorCorrection.Parent = Lighting
    
    local bloom = Instance.new("BloomEffect")
    bloom.Intensity = 0.2
    bloom.Size = 16
    bloom.Threshold = 0.9
    bloom.Parent = Lighting
    
    CurrentShader = 5
    ShadersStatus.Text = "Current: Noir\n\nBlack and white cinematic with bloom"
    CreateNotification("üåà Shader Applied", "Noir filter activated", 2)
end

local function RevertShaders()
    local Lighting = game:GetService("Lighting")
    
    for _, effect in pairs(Lighting:GetChildren()) do
        if effect:IsA("ColorCorrectionEffect") or effect:IsA("BloomEffect") or effect:IsA("BlurEffect") or effect:IsA("Atmosphere") or effect:IsA("SunRaysEffect") then
            effect:Destroy()
        end
    end
    
    if OriginalShaders.ColorCorrection then
        OriginalShaders.ColorCorrection:Clone().Parent = Lighting
    end
    if OriginalShaders.Bloom then
        OriginalShaders.Bloom:Clone().Parent = Lighting
    end
    if OriginalShaders.Blur then
        OriginalShaders.Blur:Clone().Parent = Lighting
    end
    if OriginalShaders.Atmosphere then
        OriginalShaders.Atmosphere:Clone().Parent = Lighting
    end
    if OriginalShaders.SunRays then
        OriginalShaders.SunRays:Clone().Parent = Lighting
    end
    
    CurrentShader = 0
    ShadersStatus.Text = "Current: None Applied\n\nApply different visual effects to the game"
    CreateNotification("üåà Shaders Reverted", "All shaders removed", 2)
end

local function UpdateShaderButtons()
    local buttons = {Shader1Button, Shader2Button, Shader3Button, Shader4Button, Shader5Button}
    for i, btn in ipairs(buttons) do
        TweenService:Create(btn, TweenInfo.new(0.3), {
            BackgroundColor3 = (i == CurrentShader) and CurrentTheme.Accent or Color3.fromRGB(45, 45, 52)
        }):Play()
    end
end

Shader1Button.MouseButton1Click:Connect(function()
    if CurrentShader == 1 then
        RevertShaders()
    else
        RevertShaders()
        task.wait(0.1)
        ApplyShader1()
    end
    UpdateShaderButtons()
end)

Shader2Button.MouseButton1Click:Connect(function()
    if CurrentShader == 2 then
        RevertShaders()
    else
        RevertShaders()
        task.wait(0.1)
        ApplyShader2()
    end
    UpdateShaderButtons()
end)

Shader3Button.MouseButton1Click:Connect(function()
    if CurrentShader == 3 then
        RevertShaders()
    else
        RevertShaders()
        task.wait(0.1)
        ApplyShader3()
    end
    UpdateShaderButtons()
end)

Shader4Button.MouseButton1Click:Connect(function()
    if CurrentShader == 4 then
        RevertShaders()
    else
        RevertShaders()
        task.wait(0.1)
        ApplyShader4()
    end
    UpdateShaderButtons()
end)

Shader5Button.MouseButton1Click:Connect(function()
    if CurrentShader == 5 then
        RevertShaders()
    else
        RevertShaders()
        task.wait(0.1)
        ApplyShader5()
    end
    UpdateShaderButtons()
end)

ShaderRevertButton.MouseButton1Click:Connect(function()
    RevertShaders()
    UpdateShaderButtons()
end)

local function ToggleUI()
    MainFrame.Visible = not MainFrame.Visible
    ReopenButton.Visible = not MainFrame.Visible
    
    if MainFrame.Visible then
        ReopenPulse.Visible = false
        MainFrame.Size = UDim2.new(0, 0, 0, 0)
        MainFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
        TweenService:Create(MainFrame, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 520, 0, 680),
            Position = UDim2.new(0.5, -260, 0.5, -340)
        }):Play()
        TweenService:Create(ReopenButton, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 0, 0, 0)
        }):Play()
        CreateNotification("üëÅÔ∏è UI Visible", "Interface is now visible", 1.5)
    else
        TweenService:Create(MainFrame, TweenInfo.new(0.4, Enum.EasingStyle.Back, Enum.EasingDirection.In), {
            Size = UDim2.new(0, 0, 0, 0),
            Position = UDim2.new(0.5, 0, 0.5, 0)
        }):Play()
        task.wait(0.45)
        ReopenButton.Size = UDim2.new(0, 0, 0, 0)
        ReopenPulse.Visible = true
        TweenService:Create(ReopenButton, TweenInfo.new(0.6, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
            Size = UDim2.new(0, 70, 0, 70),
            Position = UDim2.new(0.5, -35, 0, 30)
        }):Play()
        CreateNotification("üîí UI Hidden", "Click circle or press keybind to show", 2)
    end
end

ReopenButton.MouseButton1Click:Connect(function()
    ToggleUI()
end)

ReopenButton.MouseEnter:Connect(function()
    TweenService:Create(ReopenButton, TweenInfo.new(0.3), {
        Size = UDim2.new(0, 80, 0, 80),
        Position = UDim2.new(0.5, -40, 0, 25)
    }):Play()
end)

ReopenButton.MouseLeave:Connect(function()
    TweenService:Create(ReopenButton, TweenInfo.new(0.3), {
        Size = UDim2.new(0, 70, 0, 70),
        Position = UDim2.new(0.5, -35, 0, 30)
    }):Play()
end)

UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == CurrentKeybind and not IsChangingKeybind then
        ToggleUI()
    end
end)

MainFrame.Visible = true
ReopenButton.Visible = false

UpdateJumpSlider(0.5)
UpdateClickSlider(0.2)
UpdateSpamSlider(0.01)
UpdateThemeColors()
UpdateClickTypeButtons()

TweenService:Create(MainFrame, TweenInfo.new(0.8, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
    Size = UDim2.new(0, 520, 0, 680),
    Position = UDim2.new(0.5, -260, 0.5, -340)
}):Play()

CreateNotification("‚ú® Script Loaded", "Universal Utility loaded successfully!\nPress G to toggle UI", 4)
